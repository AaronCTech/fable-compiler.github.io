{
  "private": true,
  "scripts": {
    "postinstall": "dotnet restore src",
    "server": "reload -b --dir public",
    "sass": "node-sass files/styles.sass public/css/bulma.custom.css",
    "start-all": "concurrently -k \"cd src && dotnet fable fable-splitter -- -w\" \"yarn server\" \"yarn sass -w\"",
    "build-all": "concurrently \"cd src && dotnet fable-splitter\" \"yarn sass\"",
    "publish": "gh-pages -d public -b master"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "bulma": "^0.7.1",
    "concurrently": "^4.0.1",
    "fable-splitter": "^2.0.0-beta-001",
    "fs-extra": "^7.0.0",
    "gh-pages": "^1.2.0",
    "handlebars": "^4.0.11",
    "highlight.js": "^9.12.0",
    "marked": "^0.5.0",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "reload": "^2.3.1"
  }
}
